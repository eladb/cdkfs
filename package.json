{
  "name": "projen",
  "description": "A new generation of project generators",
  "repository": {
    "type": "git",
    "url": "https://github.com/projen/projen.git"
  },
  "bin": {
    "projen": "bin/projen"
  },
  "scripts": {
    "projen": "yarn compile && node bin/projen",
    "start": "yarn run projen start",
    "compile": "jsii --silence-warnings=reserved-word --no-fix-peer-dependencies && yarn run docgen",
    "test": "jest --passWithNoTests --updateSnapshot && yarn run eslint",
    "build": "yarn run compile && yarn run test && yarn run package && yarn run readme-macros",
    "bump": "standard-version",
    "release": "yarn run bump && git push --follow-tags origin master",
    "test:watch": "jest --watch",
    "test:update": "jest --updateSnapshot",
    "projen:upgrade": "yarn upgrade -L projen && CI=\"\" yarn projen",
    "watch": "jsii -w --silence-warnings=reserved-word --no-fix-peer-dependencies",
    "package": "jsii-pacmak",
    "eslint": "eslint --ext .ts,.tsx --fix --no-error-on-unmatched-pattern src test",
    "compat": "jsii-diff npm:$(node -p \"require('./package.json').name\") -k --ignore-file .compatignore || (echo \"\nUNEXPECTED BREAKING CHANGES: add keys such as 'removed:constructs.Node.of' to .compatignore to skip.\n\" && exit 1)",
    "docgen": "jsii-docgen",
    "readme-macros": "mv README.md README.md.bak && cat README.md.bak | markmac > README.md && rm README.md.bak"
  },
  "author": {
    "name": "Elad Ben-Israel",
    "email": "benisrae@amazon.com",
    "organization": false
  },
  "devDependencies": {
    "@types/fs-extra": "^8",
    "@types/glob": "^7.1.3",
    "@types/inquirer": "^7.3.1",
    "@types/jest": "^26.0.15",
    "@types/node": "^10.17.0",
    "@types/semver": "^7.3.4",
    "@types/yargs": "^15.0.10",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "eslint": "^7.14.0",
    "eslint-import-resolver-node": "^0.3.4",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "jest": "^26.6.3",
    "jest-junit": "^12",
    "jsii": "^1.15.0",
    "jsii-diff": "^1.15.0",
    "jsii-docgen": "^1.4.98",
    "jsii-pacmak": "^1.15.0",
    "jsii-release": "^0.2.3",
    "json-schema": "^0.2.5",
    "markmac": "^0.1.23",
    "standard-version": "^9.0.0",
    "ts-jest": "^26.4.4",
    "typescript": "^3.9.5"
  },
  "peerDependencies": {},
  "dependencies": {
    "@iarna/toml": "^2.2.5",
    "chalk": "^4.1.0",
    "decamelize": "^4.0.0",
    "fs-extra": "^9.0.1",
    "glob": "^7",
    "inquirer": "^7.3.3",
    "semver": "^7.3.2",
    "yaml": "^1.10.0",
    "yargs": "^16.1.1"
  },
  "bundledDependencies": [
    "@iarna/toml",
    "chalk",
    "decamelize",
    "fs-extra",
    "glob",
    "inquirer",
    "semver",
    "yaml",
    "yargs"
  ],
  "keywords": [],
  "engines": {
    "node": ">= 10.17.0"
  },
  "main": "lib/index.js",
  "license": "Apache-2.0",
  "version": "0.5.6",
  "jest": {
    "clearMocks": true,
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "testMatch": [
      "**/__tests__/**/*.ts?(x)",
      "**/?(*.)+(spec|test).ts?(x)"
    ],
    "reporters": [
      "default",
      [
        "jest-junit",
        {
          "outputDirectory": "test-reports"
        }
      ]
    ],
    "preset": "ts-jest",
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.jest.json"
      }
    }
  },
  "types": "lib/index.d.ts",
  "stability": "experimental",
  "jsii": {
    "outdir": "dist",
    "targets": {},
    "tsc": {
      "outDir": "lib",
      "rootDir": "src"
    }
  }
}