        with:
          app_id: \${{ secrets.PROJEN_APP_ID }}
          private_key: \${{ secrets.PROJEN_APP_PRIVATE_KEY }}
      - uses: hmarr/auto-approve-action@v2.2.1
        with:
          github-token: \${{ steps.generate_token.outputs.token }}
"
`;
